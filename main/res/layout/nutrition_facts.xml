        android:layout_width="wrap_content"
        android:layout_height="@dimen/nutrition_facts_vitamin_c_height"
        android:textSize="@dimen/nutrition_facts_vitamin_c_textsize"
        android:textColor="@color/nutritionFacts" />
    <TextView
        android:text="   "
        android:id="@+id/below_vitamin_c"
        android:background="@color/nutritionFacts"
        android:layout_below="@id/vitamin_c_label"
        android:textStyle="bold"
        android:layout_marginTop="@dimen/nutrition_facts_vertical_margin"
        android:layout_width="match_parent"
        android:layout_height="@dimen/nutrition_facts_rule_thin" />

    <TextView
        android:text="@string/nutrition_facts_calcium_label"
        android:id="@+id/calcium_label"
        android:layout_below="@id/below_vitamin_c"
        android:layout_marginTop="@dimen/nutrition_facts_vertical_margin"
        android:layout_marginLeft="@dimen/nutrition_facts_horizontal_margin"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/nutrition_facts_calcium_height"
        android:textSize="@dimen/nutrition_facts_calcium_label_textsize"
        android:textColor="@color/nutritionFacts"
        />
    <TextView
        android:text="15%"
        android:id="@+id/calcium"
        android:layout_below="@id/below_vitamin_c"
        android:layout_toRightOf="@id/vitamin_a_label"
        android:layout_marginTop="@dimen/nutrition_facts_vertical_margin"
        android:layout_marginLeft="@dimen/nutrition_facts_horizontal_margin"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/nutrition_facts_calcium_height"
        android:textSize="@dimen/nutrition_facts_calcium_textsize"
        android:textColor="@color/nutritionFacts"
        />
    <TextView
        android:text="@string/nutrition_facts_iron_label"
        android:id="@+id/iron_label"
        android:layout_below="@id/below_vitamin_c"
        android:layout_toLeftOf="@+id/iron"
        android:layout_marginTop="@dimen/nutrition_facts_vertical_margin"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/nutrition_facts_iron_height"
        android:textSize="@dimen/nutrition_facts_iron_label_textsize"
        android:textColor="@color/nutritionFacts" />
    <TextView
        android:text="4%"
        android:id="@+id/iron"
        android:layout_below="@id/below_vitamin_c"
        android:layout_alignParentRight="true"
        android:layout_marginTop="@dimen/nutrition_facts_vertical_margin"
        android:layout_marginLeft="@dimen/nutrition_facts_horizontal_margin"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/nutrition_facts_iron_height"
        android:textSize="@dimen/nutrition_facts_iron_textsize"
        android:textColor="@color/nutritionFacts" />
    <TextView
        android:text="   "
        android:id="@+id/below_iron"
        android:background="@color/nutritionFacts"
        android:layout_below="@id/iron_label"
        android:textStyle="bold"
        android:layout_marginTop="@dimen/nutrition_facts_vertical_margin"
        android:layout_width="match_parent"
        android:layout_height="@dimen/nutrition_facts_rule_thin" />

    <TextView
        android:text="@string/nutrition_facts_daily_comments_value"
        android:id="@+id/daily_comments"
        android:layout_below="@id/below_iron"
        android:layout_marginTop="@dimen/nutrition_facts_vertical_margin"
        android:layout_marginLeft="@dimen/nutrition_facts_horizontal_margin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/nutrition_facts_daily_value_comments_textsize"
        android:textColor="@color/nutritionFacts" />

