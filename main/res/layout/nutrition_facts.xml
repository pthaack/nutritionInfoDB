        android:layout_height="@dimen/nutrition_facts_vitamin_a_height"
        android:textSize="@dimen/nutrition_facts_vitamin_a_label_textsize"
        android:textColor="@color/nutritionFacts"
        />
    <TextView
        android:text="4%"
        android:id="@+id/vitamin_a"
        android:layout_below="@id/below_protein"
        android:layout_alignRight="@+id/calcium"
        android:layout_marginTop="@dimen/nutrition_facts_vertical_margin"
        android:layout_marginLeft="@dimen/nutrition_facts_horizontal_margin"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/nutrition_facts_vitamin_a_height"
        android:textSize="@dimen/nutrition_facts_vitamin_a_textsize"
        android:textColor="@color/nutritionFacts"
        />
    <TextView
        android:text="@string/nutrition_facts_vitamin_c_label"
        android:id="@+id/vitamin_c_label"
        android:layout_below="@id/below_protein"
        android:layout_toLeftOf="@+id/vitamin_c"
